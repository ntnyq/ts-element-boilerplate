{
  "name": "ts-element-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm run lint:style && npm run lint:script",
    "lint:script": "vue-cli-service lint",
    "lint:style": "stylelint --fix \"src/**/*.{scss,vue}\"",
    "test:unit": "vue-cli-service test:unit",
    "coverage": "vue-cli-service test:unit --coverage",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "js-cookie": "^2.2.1",
    "qs": "^6.9.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-clipboard2": "^0.3.1",
    "vue-i18n": "^8.14.1",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/js-cookie": "^2.2.2",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^6.5.1",
    "eslint-config-goy": "^0.0.5",
    "eslint-plugin-vue": "^5.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "lint-staged": "^9.4.1",
    "plop": "^2.4.0",
    "sass": "^1.18.0",
    "sass-loader": "^8.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "stylelint-webpack-plugin": "^1.0.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ],
    "!(_variables).{scss,vue}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "main": "index.js",
  "author": "ntnyq <ntnyq13@gmail.com> (https://ntnyq.com)",
  "license": "MIT"
}
