{
  "name": "ts-element-boilerplate",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "author": "ntnyq <ntnyq13@gmail.com> (https://ntnyq.com)",
  "scripts": {
    "build": "vue-cli-service build",
    "coverage": "vue-cli-service test:unit --coverage",
    "lint": "npm run lint:style && npm run lint:script",
    "lint:script": "vue-cli-service lint",
    "lint:style": "stylelint --fix \"src/**/*.{scss,vue}\"",
    "serve": "vue-cli-service serve",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test": "vue-cli-service test:unit"
  },
  "lint-staged": {
    "!(_variables).{scss,vue}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.1.2",
    "element-ui": "^2.4.5",
    "js-cookie": "^2.2.1",
    "qs": "^6.9.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-clipboard2": "^0.3.1",
    "vue-i18n": "^8.15.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.6",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/js-cookie": "^2.2.4",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-config-goy": "^0.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.4.2",
    "plop": "^2.5.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "sort-package-json": "^1.22.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "stylelint-webpack-plugin": "^1.0.2",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
